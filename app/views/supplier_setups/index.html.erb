<% @title="Supplier Setups" %>
<p id="notice"><%= notice %></p>

<div class="col-md-12">
  <div class="box">
    <div class="box-header with-border">
      <h1 class="box-title">Supplier Setup Listings</h1>
    
      <h3 class="box-title pull-right ">
        <%= link_to 'Create Cost setup', new_supplier_setup_path, class: "btn btn-warning btn-sm" %>
      </h3>
    </div>
    <div class="box-body">
      <table class="table table-bordered">
        <tr>
          <th>Supplier name</th>
          <th>Description</th>
          <th>Address</th>
          <th>Banking details</th>
          <th>Bus type</th>
          <th>Bus type</th>
          <th>Country</th>
          <th>Country name</th>
          <th>Bus clasification</th>
          <th>Notes</th>
          <th colspan="2"></th>
        </tr>
        <% @supplier_setups.each do |supplier_setup| %>
        <tr>
          <td><%= supplier_setup.supplier_name %></td>
          <td><%= supplier_setup.description %></td>
          <td><%= supplier_setup.address %></td>
          <td><%= supplier_setup.banking_details %></td>
          <td><%= supplier_setup.bus_type %></td>
          <td><%= supplier_setup.bus_type %></td>
          <td><%= supplier_setup.country_id %></td>
          <td><%= supplier_setup.country_name %></td>
          <td><%= supplier_setup.bus_clasification %></td>
          <td><%= supplier_setup.notes %></td>
          <td><%= link_to 'Show', supplier_setup %></td>
          <td><%= link_to 'Delete', supplier_setup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
      </table>
    </div><!-- /.box-body -->
    <div class="box-footer clearfix">
      <ul class="pagination pagination-sm no-margin pull-right">
        <li><%= will_paginate @insurances, renderer: BootstrapPagination::Rails %></li>
      </ul>
    </div>
  </div><!-- /.box -->
</div><!-- /.col -->

