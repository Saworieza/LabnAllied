<%= form_for(@supplier_setup) do |f| %>
  <% if @supplier_setup.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@supplier_setup.errors.count, "error") %> prohibited this supplier_setup from being saved:</h2>

      <ul>
      <% @supplier_setup.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <label for="exampleInputFile">Supplier Name</label>
    <%= f.text_field :supplier_name, :step => 'any', class: 'form-control' %>
  </div>
  
  <div class="form-group">
    <label for="exampleInputFile">Description</label>
    <%= f.text_area :description, :step => 'any', class: 'form-control' %>
  </div>
  
  <div class="form-group">
    <label for="exampleInputFile">Address</label>
    <%= f.text_area :address, :step => 'any', class: 'form-control' %>
  </div>
  
  <div class="form-group">
    <label for="exampleInputFile">Banking Details</label>
    <%= f.text_area :banking_details, :step => 'any', class: 'form-control' %>
  </div>
  
  <div class="form-group">
    <label for="exampleInputFile">Bus Type</label>
    <%= f.number_field :bus_type, :step => 'any', class: 'form-control' %>
  </div>
  
  <div class="form-group">
    <label for="exampleInputFile">Country ID</label>
    <%= f.number_field :country_id, :step => 'any', class: 'form-control' %>
  </div>
  
  <div class="form-group">
    <label for="exampleInputFile">Country Name</label>
    <%= f.text_field :country_name, :step => 'any', class: 'form-control' %>
  </div>
  
  <div class="form-group">
    <label for="exampleInputFile">Bus Classification</label>
    <%= f.number_field :bus_clasification, :step => 'any', class: 'form-control' %>
  </div>
 
  <div class="form-group">
    <label for="exampleInputFile">Notes</label>
    <%= f.text_area :notes, :step => 'any', class: 'form-control' %>
  </div>
  
  <div class="box-footer">
    <%= f.submit "Submit", class: "btn btn-success btn-sm" %>
      <!--<button type="submit" class="btn btn-primary">Submit</button>-->
  </div>
<% end %>
