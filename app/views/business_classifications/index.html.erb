<% @title="Business Classifications" %>
<p id="notice"><%= notice %></p>


<div class="col-md-12">
  <div class="box">
    <div class="box-header with-border">
      <h1 class="box-title">Business Classification Listings</h1>
    
      <h3 class="box-title pull-right ">
        <%= link_to "New Business Classification", new_business_classification_path, class: "btn btn-warning btn-sm" %>
      </h3>
    </div>
    <div class="box-body">
      <table class="table table-bordered table-hover">
        <tr>
          <th>Business classification</th>
          <th colspan="2"></th>
        </tr>
        <% @business_classifications.each do |business_classification| %>
          <tr>
            <td><%= business_classification.business_classification %></td>
            <td><%= link_to 'Show', business_classification %></td>
            <td><%= link_to 'Delete', business_classification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </table>
    </div><!-- /.box-body -->
    <div class="box-footer clearfix">
      <ul class="pagination pagination-sm no-margin pull-right">
        <li><%= will_paginate @business_classifications, renderer: BootstrapPagination::Rails %></li>
      </ul>
    </div>
  </div><!-- /.box -->
</div><!-- /.col -->

