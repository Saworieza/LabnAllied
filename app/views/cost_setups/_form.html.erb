<%= form_for(@cost_setup) do |f| %>
  <% if @cost_setup.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@cost_setup.errors.count, "error") %> prohibited this cost_setup from being saved:</h2>

      <ul>
      <% @cost_setup.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <label for="exampleInputEmail1">Cost Code</label>
    <%= f.text_area :cost_code, :step => 'any', class: 'form-control' %>
  </div>

  <div class="form-group">
    <label for="exampleInputEmail1">Cost Type</label>
    <%= f.text_field :cost_type, :step => 'any', class: 'form-control' %>
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">Cost Type Setup</label>
    <%= f.text_field :cost_type_setup, :step => 'any', class: 'form-control' %>
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">Currency</label>
    <%= f.text_field :currency, :step => 'any', class: 'form-control' %>
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">Currency Rate</label>
    <%= f.text_field :currency_rate, :step => 'any', class: 'form-control' %>
  </div>
 <div class="box-footer">
    <%= f.submit "Submit", class: "btn btn-success btn-sm" %>
      <!--<button type="submit" class="btn btn-primary">Submit</button>-->
  </div> 
<% end %>
