<% @title="Customs Service Departments" %>
<p id="notice"><%= notice %></p>


<div class="col-md-12">
  <div class="box">
    <div class="box-header with-border">
      <h1 class="box-title">Custom Service Departments</h1>
    
      <h3 class="box-title pull-right ">
        <%#= link_to new_customs_service_department_path, class: "btn btn-info" do %>
        <%= link_to 'Add Custom Service Department', new_customs_service_department_path, class: "btn btn-warning btn-sm" %>
      </h3>
    </div>
    <div class="box-body">
      <table class="table table-bordered table-hover">
        <tr>
          <th>Idf form nos</th>
          <th>Payment date</th>
          <th>Rate</th>
          <th colspan="2"></th>
        </tr>
        <% @customs_service_departments.each do |customs_service_department| %>
          <tr>
            <td><%= customs_service_department.idf_form_nos %></td>
            <td><%= customs_service_department.payment_date %></td>
            <td><%= customs_service_department.rate %></td>
            <td><%= link_to 'Show', customs_service_department %></td>
            <td><%= link_to 'Delete', customs_service_department, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </table>
    </div><!-- /.box-body -->
    <div class="box-footer clearfix">
      <ul class="pagination pagination-sm no-margin pull-right">
        <li><%= will_paginate @custom_service_departments, renderer: BootstrapPagination::Rails %></li>
      </ul>
    </div>
  </div><!-- /.box -->
</div><!-- /.col -->






