<%= form_for(@file_information) do |f| %>
  <% if @file_information.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@file_information.errors.count, "error") %> prohibited this file_information from being saved:</h2>

      <ul>
      <% @file_information.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <label for="exampleInputFile">File Number</label>
    <%= f.text_field :file_number, :step => 'any', class: 'form-control' %>
  </div>

  <div class="form-group">
    <label>IDF Date:</label>
    <div class="input-group">
      <div class="input-group-addon">
        <i class="fa fa-calendar"></i>
      </div>
      <%= f.date_select :idf_date %>
      <%#= f.date_select :application_date, :step => 'any', class: 'form-control' %>
      <!--<input class="form-control" data-inputmask="'alias': 'dd/mm/yyyy'" data-mask="" type="text">-->
    </div><!-- /.input group -->
  </div>

  
  <div class="box-footer">
    <%= f.submit "Submit", class: "btn btn-success btn-sm" %>
      <!--<button type="submit" class="btn btn-primary">Submit</button>-->
  </div> 
<% end %>
