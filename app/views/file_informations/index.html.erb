<p id="notice"><%= notice %></p>

<div class="col-md-12">
  <div class="box">
    <div class="box-header with-border">
      <h1 class="box-title">File Information Listings</h1>
    
      <h3 class="box-title pull-right ">
        <%= link_to "New File Information", new_file_information_path, class: "btn btn-warning btn-sm" %>
      </h3>
    </div>
    <div class="box-body">
      <table class="table table-bordered table-hover">
        <tr>
          <th>File number</th>
          <th>Idf date</th>
          <th colspan="2"></th>
        </tr>
        <% @file_informations.each do |file_information| %>
          <tr>
            <td><%= file_information.file_number %></td>
            <td><%= file_information.idf_date %></td>
            <td><%= link_to 'Show', file_information %></td>
            <td><%= link_to 'Delete', file_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </table>
    </div><!-- /.box-body -->
    <div class="box-footer clearfix">
      <ul class="pagination pagination-sm no-margin pull-right">
        <li><%= will_paginate @file_informations, renderer: BootstrapPagination::Rails %></li>
      </ul>
    </div>
  </div><!-- /.box -->
</div><!-- /.col -->


<h1>Listing File Informations</h1>

<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>File number</th>
      <th>Idf date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @file_informations.each do |file_information| %>
      <tr>
        <td><%= file_information.file_number %></td>
        <td><%= file_information.idf_date %></td>
        <td><%= link_to 'Show', file_information %></td>
        <td><%= link_to 'Edit', edit_file_information_path(file_information) %></td>
        <td><%= link_to 'Destroy', file_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New File information', new_file_information_path %>
