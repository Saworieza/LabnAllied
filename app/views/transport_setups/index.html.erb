<% @title="Transport Setups" %>
<p id="notice"><%= notice %></p>


<div class="col-md-12">
  <div class="box">
    <div class="box-header with-border">
      <h1 class="box-title">Transport Setup Listings</h1>
    
      <h3 class="box-title pull-right ">
        <%= link_to 'Create a Transport Setup', new_transport_setup_path, class: "btn btn-warning btn-sm" %>
      </h3>
    </div>
    <div class="box-body">
      <table class="table table-bordered">
        <tr>
          <th>Transport mode</th>
          <th>Port of loading</th>
          <th>Port of dischange</th>
          <th>Final destination</th>
          <th>Clearing agent</th>
          <th>Container type</th>
          <th colspan="2"></th>
        </tr>
        <tr>
          <% @transport_setups.each do |transport_setup| %>
            <tr>
              <td><%= transport_setup.transport_mode %></td>
              <td><%= transport_setup.port_of_loading %></td>
              <td><%= transport_setup.port_of_dischange %></td>
              <td><%= transport_setup.final_destination %></td>
              <td><%= transport_setup.clearing_agent %></td>
              <td><%= transport_setup.container_type %></td>
              <td><%= link_to 'Show', transport_setup %></td>
              <td><%= link_to 'Delete', transport_setup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tr>
      </table>
    </div><!-- /.box-body -->
    <div class="box-footer clearfix">
      <ul class="pagination pagination-sm no-margin pull-right">
        <li><%= will_paginate @transport_setups, renderer: BootstrapPagination::Rails %></li>
      </ul>
    </div>
  </div><!-- /.box -->
</div><!-- /.col -->




