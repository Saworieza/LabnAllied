<%= form_for(@transport_setup) do |f| %>
  <% if @transport_setup.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@transport_setup.errors.count, "error") %> prohibited this transport_setup from being saved:</h2>

      <ul>
      <% @transport_setup.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>


  <div class="form-group">
    <label for="exampleInputFile">Transport Mode</label>
    <%= f.text_field :transport_mode, :step => 'any', class: 'form-control' %>
  </div>
  
  <div class="form-group">
    <label for="exampleInputFile">Port Of Loading</label>
    <%= f.text_field :port_of_loading, :step => 'any', class: 'form-control' %>
  </div>
  
  <div class="form-group">
    <label for="exampleInputFile">Port Of Discharge</label>
    <%= f.text_field :port_of_dischange, :step => 'any', class: 'form-control' %>
  </div>
  
  <div class="form-group">
    <label for="exampleInputFile">Final Destination</label>
    <%= f.text_field :final_destination, :step => 'any', class: 'form-control' %>
  </div>
  
  <div class="form-group">
    <label for="exampleInputFile">Clearing Agent</label>
    <%= f.text_field :clearing_agent, :step => 'any', class: 'form-control' %>
  </div>
  
  <div class="form-group">
    <label for="exampleInputFile">Container Type</label>
    <%= f.text_field :container_type, :step => 'any', class: 'form-control' %>
  </div>
  <div class="box-footer">
    <%= f.submit "Submit", class: "btn btn-success btn-sm" %>
      <!--<button type="submit" class="btn btn-primary">Submit</button>-->
  </div>
<% end %>
