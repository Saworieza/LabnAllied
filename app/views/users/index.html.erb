<% @title="Users" %>
<div class="row" style="a{color: #000 important!;}">
   	<div class="col-lg-12">
		<div class="box box-primary col-lg-6">
		  <div class="box-header with-border ">
		    <h3 class="box-title">Listing Users </h3>
		  </div>
		  <div class="box-body">
			<table class="table table-hover table-bordered no-wrap">
				<thead>
                	<tr>
                    	<th>#</th>
                    	<th>Username</th>
                    	<th>First Name</th>
                    	<th>Last Name</th>
                    	<th>Email</th>
                    	<th>Role</th>
                    	<th>Edit</th>
                	</tr>
            	</thead>
				<tbody>
					<% @users.each do |user| %>
						<tr><%= render user %></tr>
					<% end %>
				</tbody>
			</table>
		  </div>
		  <div class="box-footer clearfix">
		    <ul class="pagination pagination-sm no-margin pull-right">
		      <li><%= will_paginate @transport_setups, renderer: BootstrapPagination::Rails %></li>
		    </ul>
		  </div>
		</div>
	</div>
</div>
	
